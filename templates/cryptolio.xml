<?xml version="1.0"?>
<Container version="2">
  <Name>Cryptolio</Name>
  <Repository>xtrendence/cryptofolio</Repository>
  <Registry>https://hub.docker.com/r/xtrendence/cryptofolio/</Registry>
  <Branch>
  <Tag>latest</Tag>
  <TagDescription>Latest release of cryptofolio</TagDescription>
  </Branch>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/Xtrendence/Cryptofolio</Project>
  <Overview>Crypto Portfolio</Overview>
  <Category>Crypto: Cryptocurrency:</Category>
  <WebUI>http://[IP]:[PORT:8180]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/ziggyds/unraid-templates/master/templates/cryptolio.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/ziggyds/unraid-templates/master/templates/cryptolio-logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateLink>https://github.com/ziggyds</DonateLink>
  <DonateText>If you like it please consider donating.</DonateText>
  <Description>Crypto Portfolio</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8180</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/cryptolio</HostDir>
      <ContainerDir>/var/www/html</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
</Container>